#include<stdio.h>
int main()
{
	int n,m,l;
	printf("enter the length of a number:");
	scanf("%d",&l);
	printf("enter the digit of a num:");
	scanf("%d",&n);
	m=n;
	int sum=0;
	while(n!=0)
	{
		int digit=n%10;
		sum+=pow(digit,l);
		n=n/10;
	}
	if(sum==m)
	{
		printf("it is a armstrong");
	}
	else
	{
		printf("is is a not a armstrong");
	}
}
